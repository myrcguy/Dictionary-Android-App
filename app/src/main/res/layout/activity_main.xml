<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/FrequentOutput3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginRight="30dp"
            android:layout_marginEnd="30dp" android:textSize="16sp" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/FrequentOutput2"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/FrequentOutput2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginRight="30dp"
            android:layout_marginEnd="30dp" android:textSize="16sp" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/FrequentOutput1"/>
    <TextView
            android:text="Frequent Word 3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Frequent3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" android:layout_marginEnd="280dp" android:layout_marginRight="280dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ButtonFind" android:layout_marginTop="118dp"
            android:textSize="16sp"/>
    <Button
            android:text="NEW WORD"
            android:layout_width="262dp"
            android:layout_height="55dp" android:id="@+id/ButtonNext"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="70dp" android:layout_marginStart="70dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="70dp" android:layout_marginRight="70dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="300dp"/>
    <Button
            android:text="REMOVE"
            android:layout_width="262dp"
            android:layout_height="55dp" android:id="@+id/ButtonRemove"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="70dp" android:layout_marginStart="70dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="70dp" android:layout_marginRight="70dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="400dp"/>
    <Button
            android:text="FIND"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ButtonFind"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="18dp" android:layout_marginStart="18dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="66dp" app:layout_constraintEnd_toStartOf="@+id/TypeWord"
            android:layout_marginEnd="22dp" android:layout_marginRight="22dp"/>
    <TextView
            android:text="Enter Your Word"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/EnterYourWord"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="30dp" android:textSize="24sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="125dp" android:layout_marginStart="125dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/TypeWord"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="70dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="70dp"
            android:layout_marginEnd="70dp"/>
    <TextView
            android:text="Frequent Word 1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Frequent1"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" android:layout_marginEnd="280dp" android:layout_marginRight="280dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/ButtonFind" android:textSize="16sp"/>
    <TextView
            android:text="Frequent Word 2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Frequent2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" android:layout_marginEnd="280dp" android:layout_marginRight="280dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="85dp"
            app:layout_constraintTop_toBottomOf="@+id/ButtonFind" android:textSize="16sp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/FrequentOutput1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginRight="30dp"
            android:layout_marginEnd="30dp" android:textSize="16sp" android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/TypeWord"/>
    <TextView
            android:layout_width="299dp"
            android:layout_height="207dp" android:id="@+id/outputDefinition"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="80dp"
            android:layout_marginStart="80dp" android:layout_marginEnd="224dp" android:layout_marginRight="224dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/FrequentOutput3" android:textSize="20sp"
            app:layout_constraintHorizontal_bias="0.14"/>

</androidx.constraintlayout.widget.ConstraintLayout>